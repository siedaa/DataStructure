#include <iostream>
#include <stack>
using namespace std;

int main() {
    string exp;
    cout << "Enter Postfix Expression: ";
    cin >> exp;

    stack<int> st;

    for (int i = 0; i < exp.length(); i++) {
        char ch = exp[i];

        // If character is a digit, push onto stack
        if (isdigit(ch)) {
            st.push(ch - '0');  // Convert char to int
        }
        else {
            // Pop two operands
            int val2 = st.top(); st.pop();
            int val1 = st.top(); st.pop();
            int result;

            // Perform operation
            switch(ch) {
                case '+': result = val1 + val2; break;
                case '-': result = val1 - val2; break;
                case '*': result = val1 * val2; break;
                case '/': result = val1 / val2; break;
                default:
                    cout << "Invalid Operator!" << endl;
                    return 0;
            }

            // Push result back to stack
            st.push(result);
        }
    }

    // Final result
    cout << "Result: " << st.top() << endl;

    return 0;
}
