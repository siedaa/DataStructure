#include <iostream>
#include <vector>
using namespace std;

class DynamicArray {
public:
    vector<int> data;

public:
    // Add element at the end
    void push_back(int value) {
        data.push_back(value);
    }

    // Print all elements
    void print() const {
        for (int i = 0; i < data.size(); i++) {
            cout << data[i] << " ";
        }
        cout << endl;
    }

    // Get current size
    int getSize() const {
        return data.size();
    }

    // Get current capacity
    int getCapacity() const {
        return data.capacity();
    }
};

int main() {
    DynamicArray d1;
    d1.push_back(12);
    d1.push_back(19);
    d1.push_back(13);

    d1.print();

    cout << "Size: " << d1.getSize() << endl;
    cout << "Capacity: " << d1.getCapacity() << endl;


    d1.push_back(122);
    d1.push_back(198);
    d1.push_back(134);

    d1.print();

    cout << "Size: " << d1.getSize() << endl;
    cout << "Capacity: " << d1.getCapacity() << endl;

    return 0;
}
